{% extends "base" %}

{% block content %}
    <h1>The number of images is: {{ num_files }}!</h1>
    <h3>The images are:</h3>
    <ul>
        {% for file in files %}
            <li>{{ file }}</li>
        {% endfor %}
    </ul>

    <p>Try going to <a href="hello/Your%20Name">/hello/Your Name</a></p>
{% endblock content %}